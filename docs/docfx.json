{
  "metadata": [
    {
      "src": [
        {
          "src": "../src/",
          "files": [
            "Csra/**/*.csproj",
            "Csra_TestMethods/**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "toc.yml",
          "principles/**/*.{md,yml}",
          "features/**/*.{md,yml}",
          "releases_/**/*.{md,yml}",
          "pdf/**/*.{md,yml}",
          "getting-started/**/*.{md,yml}",
          "training/**/*.{md,yml}",
          "api/**/*.{md,yml}"
        ],
        "exclude":[
          "_site",
          "training/slidev/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "overwrite/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "fileMetadata": {
      "pdf": {
        "pdf/toc.yml": true
      },
      "pdfFileName": {
        "pdf/toc.yml": "csra_documentation.pdf"
      }
    },
    "dest": "_site",
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_custom_overview": "Overview",
      "_custom_details": "Details",
      "_custom_examples": "Examples",
      "_appTitle": "C# Reference Architecture",
      "_appLogoPath": "media/logo.png",
      "_appFooter": "<span>Â© 2024 - <span id='currentYear'></span> Teradyne - Generated by <a href=\"https://dotnet.github.io/docfx\">DocFx</a></span>",
      "_disableBreadcrumb": false,
      "_enableSearch": true,
      "_disableContribution": true,
      "_enableGitContribute": false
    },
    "template": [
      "templates/default",
      "templates/modern",
      "templates/custom_template"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
